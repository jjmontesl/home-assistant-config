proc            /proc           proc    defaults          0       0
/dev/mmcblk0p6  /boot           vfat    defaults          0       2
/dev/mmcblk0p7  /               ext4    defaults,noatime  0       1

tmpfs           /srv/home-assistant/tmp  tmpfs  nodev,nosuid,size=4M  0  0

